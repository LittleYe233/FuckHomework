{
  "name": "fuckhomework",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test:lint": "eslint '**/*.{ts,js,cjs,svelte}'",
    "test:lint:fix": "eslint '**/*.{ts,js,cjs,svelte}' --fix",
    "test:prettier": "prettier-check '**/*.{json,html}'",
    "test:prettier:fix": "prettier '**/*.{json,html}' --write",
    "format:check": "npm run test:lint && npm run test:prettier",
    "format:staged": "npm run test:lint:fix && pretty-quick --staged --verbose --pattern '**/*.{json,html}'"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.96",
    "@sveltejs/kit": "^1.0.0-next.511",
    "@types/cookie": "^0.5.1",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "i18next-fs-backend": "^1.1.5",
    "lodash": "^4.17.21",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-check": "^2.0.0",
    "prettier-plugin-svelte": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "svelte": "^3.46.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "yaml": "^2.1.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "cookie": "^0.4.1",
    "i18next": "^21.9.1",
    "utility-types": "^3.10.0"
  }
}
