{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "~/*": ["./*"],
      "$lib": ["./lib"],
      "$lib/*": ["./lib/*"]
    },
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts", "config.js"]
}
